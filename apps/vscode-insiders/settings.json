{
    "telemetry.feedback.enabled": false,
    "telemetry.telemetryLevel": "off",
    "gitlens.telemetry.enabled": false,
    "terminal.integrated.fontLigatures.enabled": true,
    "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font, JetBrainsMono Nerd Font Mono, FiraCode Nerd Font Mono, MesloLGL Nerd Font, CaskaydiaCove Nerd Font, Geist Mono, Menlo, Monaco, PT Mono",
    "workbench.colorTheme": "Houston",
    "files.autoSave": "onFocusChange",
    "editor.fontSize": 13,
    "editor.fontFamily": "FiraCode Nerd Font Mono, Hack Nerd Font Mono, JetBrainsMonoNL Nerd Font Mono, MesloLGL Nerd Font Mono, Office Code Pro, Menlo, Monaco, 'Courier New', monospace",
    "editor.wordWrap": "wordWrapColumn",
    "totalTypeScript.hideAllTips": false,
    "totalTypeScript.hideBasicTips": false,
    "workbench.activityBar.location": "top",
    "workbench.cloudChanges.autoResume": "off",
    "workbench.cloudChanges.continueOn": "off",
    "workbench.enableExperiments": false,
    "typescript.check.npmIsInstalled": false,
    "autoBarrel.enable": false,
    "chat.sendElementsToChat.enabled": false,
    "bookmarks.label.suggestion": "suggestWhenSelectedOrLineWhenNoSelected",
    "bookmarks.useWorkaroundForFormatters": true,
    "bun.diagnosticsSocket.enabled": false,
    "eslint.codeActionsOnSave.mode": "problems",
    "eslint.ignoreUntitled": true,
    "git.autofetch": true,
    "github.copilot.enable": {
        "plaintext": true,
        "markdown": true,
        "scminput": true
    },
    "github.copilot.chat.localeOverride": "en",
    "gitlens.currentLine.enabled": false,
    "gitlens.currentLine.pullRequests.enabled": false,
    "gitlens.codeLens.enabled": false,
    "gitlens.codeLens.authors.enabled": false,
    "errorLens.codeLensEnabled": true,
    "github.copilot.nextEditSuggestions.enabled": true,
    "github.copilot.chat.codesearch.enabled": true,
    "github.copilot.chat.agent.thinkingTool": true,
    "github.copilot.chat.editor.temporalContext.enabled": true,
    "github.copilot.chat.edits.temporalContext.enabled": true,
    "github.copilot.chat.generateTests.codeLens": true,
    "github.copilot.chat.languageContext.typescript.enabled": true,
    "github.copilot.chat.newWorkspace.useContext7": true,
    "githubRepositories.autoFetch.interval": 480,
    "githubRepositories.experimental.prefetch": false,
    "githubRepositories.indexing.verboseDownloadNotification": false,
    "githubRepositories.lfs.read.enabled": false,
    "githubRepositories.lfs.write.enabled": false,
    "gitlens.plusFeatures.enabled": false,
    "vscode-edge-devtools.browserFlavor": "Dev",
    "vscode-edge-devtools.webhint": false,
    "simpleBrowser.focusLockIndicator.enabled": false,
    "extensions.closeExtensionDetailsOnViewChange": true,
    "extensions.experimental.issueQuickAccess": false,
    "extensions.experimental.deferredStartupFinishedActivation": true,
    "extensions.verifySignature": false,
    "terminal.explorerKind": "both",
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.enablePersistentSessions": false,
    "terminal.integrated.localEchoEnabled": "auto",
    "terminal.integrated.shellIntegration.decorationsEnabled": "gutter",
    "terminal.integrated.tabs.defaultColor": "terminal.ansiYellow",
    "terminal.integrated.tabs.defaultIcon": "device-mobile",
    "workbench.panel.showLabels": false,
    "terminal.integrated.cursorStyle": "underline",
    "terminal.integrated.cursorStyleInactive": "block",
    "terminal.integrated.defaultProfile.osx": "zsh",
    "terminal.integrated.hideOnLastClosed": false,
    "terminal.integrated.persistentSessionReviveProcess": "never",
    "terminal.integrated.rightClickBehavior": "default",
    "terminal.integrated.suggest.inlineSuggestion": "alwaysOnTopExceptExactMatch",
    "terminal.integrated.tabs.showActions": "always",
    "terminal.integrated.tabs.showActiveTerminal": "always",
    "terminal.integrated.tabs.hideCondition": "never",
    "workbench.editorAssociations": {
        "*.copilotmd": "vscode.markdown.preview.editor",
        "*.png": "imagePreview.previewEditor"
    },
    "git.enableSmartCommit": true,
    "git.confirmSync": false
}